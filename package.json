    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jquery": "^2.2.4",
    "jquery-flesler-scrollto": "^2.1.1",
    "jquery-resizable-dom": "^0.20.0",
    "jquery-validation": "^1.17.0",
